{
    "name": "mvieira/json-collection-next",
    "description": "PHP implementation of the Collection.next+JSON Media Type",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/mickaelvieira/JsonCollectionNext",
    "minimum-stability": "stable",
    "keywords": [
        "Hypermedia",
        "RESTful",
        "HTTP", "JSON",
        "REST",
        "HATEOAS"
    ],
    "authors": [
        {
            "name": "Mickael Vieira",
            "email": "contact@mickael-vieira.com",
            "homepage": "http://mickael-vieira.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/mickaelvieira/JsonCollection.git"
        }
    ],
    "require": {
        "php": "~5.4",
        "mvieira/json-collection": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "phpspec/nyan-formatters": "1.*",
        "henrikbjorn/phpspec-code-coverage": "~0.2",
        "squizlabs/php_codesniffer": "1.*",
        "phpmd/phpmd" : "2.0.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "JsonCollectionNext\\": "src/JsonCollectionNext/"
        }
    }
}